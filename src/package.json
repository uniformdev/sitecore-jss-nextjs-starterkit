{
  "name": "@uniformdev/uniform-sitecore-jss-nextjs-starterkit",
  "version": "0.0.1",
  "private": true,
  "config": {
    "appName": "uniform-jss",
    "rootPlaceholders": [
      "uniform-jss-content"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/api/uniform-jss",
    "language": "en",
    "appLanguages": [
      "en"
    ]
  },
  "scripts": {
    "bootstrap:connected": "node scripts/bootstrap.js",
    "bootstrap:connected:export": "node scripts/bootstrap.js --export",
    "bootstrap:disconnected": "node scripts/bootstrap.js --disconnected",
    "bootstrap:disconnected:export": "node scripts/bootstrap.js --disconnected --export",    
    "build": "next build",
    "azure": "node ./deploy-azure.js",
    "aws": "node ./deploy-aws.js",
    "netlify": "node ./deploy-netlify.js",
    "deploy:azure": "npm-run-all --serial export azure",
    "deploy:aws": "npm-run-all --serial export aws",
    "deploy:netlify": "npm-run-all --serial export netlify",
    "export": "cross-env-shell NODE_ENV=production npm-run-all --serial bootstrap:connected:export build export:site",
    "export:disconnected": "cross-env-shell JSS_MODE=disconnected NODE_ENV=production APP_MODE=export npm-run-all --serial bootstrap:disconnected:export generate-manifest build export:site",
    "export:site": "next export",
    "generate-manifest": "jss manifest -c -d",
    "start": "cross-env-shell JSS_MODE=connected \"npm-run-all --serial bootstrap:connected --parallel start-server start:watch-components\"",
    "start:disconnected": "cross-env-shell JSS_MODE=disconnected \"npm-run-all --serial bootstrap:disconnected --parallel start-server start:watch-components\"",
    "start:production": "cross-env-shell JSS_MODE=connected APP_MODE=ssr NODE_ENV=production \"npm-run-all --serial bootstrap:connected build --parallel start-server start:watch-components\"",
    "start:watch-components": "node scripts/generate-component-factory.js --watch",
    "start-server": "node ./server/server.js"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss-react": "^13.0.0",
    "@sitecore-jss/sitecore-jss-tracking": "^13.0.0",
    "@uniformdev/next-server": "3.0.0-uniformdev",
    "@uniformdev/publishing-all": "3.0.0-uniformdev",
    "@zeit/next-css": "^1.0.1",
    "axios": "~0.19.0",
    "i18next": "^19.0.1",
    "loglevel": "^1.6.6",
    "next": "9.3.5",
    "next-compose-plugins": "^2.2.0",
    "next-progressbar": "^1.0.0",
    "path-to-regexp": "^6.1.0",
    "react-i18next": "^10.12.2",
    "winston": "^3.2.1"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/register": "^7.5.5",
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@sitecore-jss/sitecore-jss-cli": "^13.0.0",
    "@sitecore-jss/sitecore-jss-dev-tools": "^13.0.0",
    "@sitecore-jss/sitecore-jss-manifest": "^13.0.0",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.1.0",
    "body-parser": "^1.19.0",
    "chokidar": "^3.0.2",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^4.0.2",
    "express": "^4.17.1",
    "fs-extra": "^9.0.0",
    "graphql.macro": "^1.4.2",
    "js-yaml": "^3.13.1",
    "next-offline": "^5.0.0",
    "next-plugin-graphql": "0.0.2",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "tailwindcss": "^1.2.0"
  }
}
